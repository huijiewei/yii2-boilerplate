{
  "name": "boilerplate-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.entry.admin.config.js vue-cli-service serve",
    "serve:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.entry.mobile.config.js vue-cli-service serve",
    "build:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.entry.admin.config.js vue-cli-service build",
    "build:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.entry.mobile.config.js vue-cli-service build",
    "inspect:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.entry.admin.config.js vue-cli-service inspect --mode production > webpack.entity.admin.js",
    "inspect:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=./vue.entry.mobile.config.js vue-cli-service inspect --mode production > webpack.entity.mobile.js"
  },
  "dependencies": {
    "@composer-asset/huijiewei--yii2-swagger": "file:./vendor/foxy/composer-asset/huijiewei/yii2-swagger",
    "@composer-asset/huijiewei--yii2-upload": "file:./vendor/foxy/composer-asset/huijiewei/yii2-upload",
    "@composer-asset/yiisoft--yii2": "file:./vendor/foxy/composer-asset/yiisoft/yii2",
    "@composer-asset/yiisoft--yii2-bootstrap": "file:./vendor/foxy/composer-asset/yiisoft/yii2-bootstrap",
    "axios": "^0.21.1",
    "axios-extensions": "^3.1.3",
    "axios-progress-bar": "^1.2.0",
    "core-js": "^3.8.2",
    "element-ui": "^2.15.0",
    "perfect-scrollbar": "^1.5.0",
    "query-string": "^6.13.8",
    "register-service-worker": "^1.7.2",
    "vue": "^2.6.12",
    "vue-context": "^6.0.0",
    "vue-cropperjs": "^4.2.0",
    "vue-router": "^3.4.9",
    "vue-router-back-button": "^1.3.0",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-plugin-router": "^4.5.8",
    "@vue/cli-plugin-vuex": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "assign-deep": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.12.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "less": "^4.1.0",
    "less-loader": "^7.0.2",
    "mocker-api": "^2.7.5",
    "prettier": "^2.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack-manifest-plugin": "^3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "license": "MIT"
